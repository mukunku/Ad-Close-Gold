{
  "manifest_version": 2,
  "name": "Ad Close Gold",
  "version": "1.0.1.2",
  "permissions": [
    "tabs",
	"storage"
  ],
  "options_page": "options.html",
  "description": "This extension automatically closes tabs matching certain search patterns",
  "icons": {
    "128": "icon128.png"
  },
  "author": "mukunku",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}